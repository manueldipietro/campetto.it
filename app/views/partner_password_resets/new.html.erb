<% provide(:title, "Partner password dimenticata")%>
<h1>Password dimenticata</h1>

<%= form_for(:password_reset, url: partner_password_resets_path) do |f| %>
    <%= f.label :email %>
    <%= f.email_field :email, class: 'form-control' %>
    <%= f.submit "Submit", class: "btn btn-primary" %>
<% end %>