<% provide(:title, "Partner reset della password") %>

<%= form_for(@partner, url: partner_password_reset_path(params[:id])) do |f| %>
    <!--RENDER ERRORE LISTATO 10.48 -->
    <%= hidden_field_tag :email, @partner.email %>

    <%= f.label :password %>
    <%= f.password_field :password, class: "form-control" %>

    <%= f.label :password_confirmation, "Confirmation" %>
    <%= f.password_field :password_confirmation, class: "form-control" %>

    <%= f.submit "Aggiorna la password", class: "btn btn-primary" %>
<% end %>

